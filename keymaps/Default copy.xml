<keymap version="1" name="Default copy" parent="$default">
  <action id="CodeCompletion">
    <keyboard-shortcut first-keystroke="ctrl space" />
    <keyboard-shortcut first-keystroke="ctrl comma" />
  </action>
  <action id="HideActiveWindow">
    <keyboard-shortcut first-keystroke="shift escape" />
    <keyboard-shortcut first-keystroke="ctrl q" />
  </action>
  <action id="QuickJavaDoc">
    <mouse-shortcut keystroke="alt button2" />
  </action>
</keymap>